
Bundle-SymbolicName: peaberry-lib
Bundle-Version: ${version}

Include-Resource:\
 OSGI-OPT/NOTICE=NOTICE,\
 OSGI-OPT/LICENSE=LICENSE,\
 OSGI-OPT/src=src

Export-Package:\
 org.ops4j.peaberry

Private-Package:\
 org.ops4j.peaberry.internal

Import-Package:\
 org.osgi.framework; version="[1.3.0, 2.0.0)",\
 *

-nouses: true

-removeheaders:\
 Bnd-LastModified,\
 Ignore-Package,\
 Include-Resource,\
 Private-Package,\
 Tool

-output:\
 ${extension.jar}

